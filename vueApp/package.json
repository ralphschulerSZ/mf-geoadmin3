    "build": "NODE_ENV=production webpack --config webpack.config.js && npm run build:goog && npm run build:deps",
    "build:goog": "sed -i '1s/^/goog.provide(\"ngVueComponents\");\\n/' dist/js/appVueLib_NgVueBridge.js",
